my_module.content:
  path: '/my-page' 
  defaults: 
    _controller: '\Drupal\my_module\Controller\MypageController::content' 
    _title: 'Code Examples'
  requirements: 
    _permission: 'access content'
  options:
    no_cache: 'TRUE'
